{
  "name": "epic-charts",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @epic-charts/web dev",
    "dev:bot": "pnpm --filter @epic-charts/bot dev",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter @epic-charts/shared build && pnpm --filter @epic-charts/web build",
    "build:bot": "pnpm --filter @epic-charts/bot build",
    "lint": "pnpm -r lint",
    "preview": "pnpm --filter @epic-charts/web preview"
  },
  "devDependencies": {
    "typescript": "~5.9.3"
  }
}
